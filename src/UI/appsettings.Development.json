{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Observability": {
    "HttpLogging": {
      "Enabled": true,
      "MaxBodyLength": 5000,
      "LogHeaders": true,
      "LogRequestBody": true,
      "LogResponseBody": true
    }
  }
}
